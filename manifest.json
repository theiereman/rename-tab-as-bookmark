{
  "manifest_version": 3,
  "name": "RenameTabAsBookmark",
  "version": "1.0",

  "description": "Automaticallly renames the current tab with the bookmark name.",

  "background": {
    "scripts": ["rename_tab_as_bookmark.js"],
    "type": "module"
  },

  "host_permissions": ["<all_urls>"],
  "permissions": ["tabs", "bookmarks", "scripting"]
}
